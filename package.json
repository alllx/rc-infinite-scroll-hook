{
  "name": "rc-infinite-scroll-hook",
  "version": "1.0.0",
  "description": "react infinite scroll hook",
  "main": "dist/useInfiniteScroll.js",
  "scripts": {
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watchAll",
    "lint": "tslint -p .",
    "tsc": "tsc -d",
    "start": "rm -rf example/dist && parcel ./example/src/index.html -d ./example/dist",
    "prebuild": "rm -rf dist",
    "build:example": "rm -rf example/dist && parcel build ./example/src/index.html -d ./example/dist",
    "deploy": "gh-pages -d example/dist",
    "publish-demo": "npm run build:example && npm run deploy",
    "release": "npm run lint && npm run test && npm run tsc"
  },
  "keywords": [
    "react",
    "hook",
    "infinite",
    "scroll"
  ],
  "author": "Alexander Yamkov",
  "license": "MIT",
  "homepage": "https://github.com/alllx/",
  "repository": {
    "type": "git",
    "url": "https://github.com/alllx/rc-infinite-scroll-hook.git"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.9.7",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "dependencies": {},
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
